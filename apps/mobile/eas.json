{
  "cli": {
    "version": ">= 8.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "http://192.168.1.62:3001",
        "EXPO_PUBLIC_DEV_MODE": "true",
        "NODE_ENV": "development"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.happybar.app",
        "EXPO_PUBLIC_DEV_MODE": "false",
        "NODE_ENV": "production"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.happybar.app",
        "EXPO_PUBLIC_DEV_MODE": "false",
        "NODE_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
